    "cacheable-lookup": "^6.0.0",
    "common-tags": "^1.8.0",
    "connect": "^3.7.0",
    "cors": "^2.8.4",
    "cors-gate": "^1.1.3",
    "express": "^4.14.0",
    "express-graphql": "^0.11.0",
    "fetch-ponyfill": "^7.1.0",
    "graphql": "^14.0.2 || ^15.5",
    "graphql-subscriptions": "^1.1.0",
    "http-encoding": "^1.4.0",
    "http2-wrapper": "2.0.5",
    "lodash": "^4.16.4",
    "native-duplexpair": "^1.0.0",
    "node-forge": "^0.10.0",
    "performance-now": "^2.1.0",
    "portfinder": "^1.0.23",
    "typed-error": "^3.0.2",
    "uuid": "^3.1.0",
    "ws": "^7.3.1"
  }
}
